{% if supabase %}
output "supabase_project_url" {
  description = "Supabase project URL"
  value       = module.supabase.project_url
}

output "supabase_database_url" {
  description = "Supabase database connection string"
  value       = module.supabase.database_url
  sensitive   = true
}
{% endif %}
{% if cloudflare %}

output "frontend_url" {
  description = "Frontend application URL"
  value       = module.cloudflare.frontend_url
}

output "backend_url" {
  description = "Backend API URL"
  value       = module.cloudflare.backend_url
}
{% endif %}
